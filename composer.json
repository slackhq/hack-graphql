{
  "name": "slack/hack-graphql",
  "description": "GraphQL server implementation for Hack.",
  "type": "library",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Michael Hahn",
      "email": "mh@slack-corp.com"
    }
  ],
  "require": {
    "hhvm/hsl": "^4.41.0",
    "slack/hack-graphql-common": "v0.1",
    "slack/hack-graphql-tokenizer": "v0.1",
    "slack/hack-graphql-parser": "v0.3"
  },
  "require-dev": {
    "hhvm/hhast": "^4.53.4",
    "facebook/fbexpect": "^2.7.4",
    "hhvm/hacktest": "^2.1.2"
